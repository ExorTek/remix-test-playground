{
  "name": "remix-test-playground",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "dev:c:fastify": "cross-env NODE_ENV=development node ./fastify-server.cjs",
    "dev:c:express": "cross-env NODE_ENV=development node  ./express-server.cjs",
    "start:c:fastify": "cross-env NODE_ENV=production node ./fastify-server.cjs",
    "start:c:express": "cross-env NODE_ENV=production node ./express-server.cjs",
    "dev:m:fastify": "cross-env NODE_ENV=development node ./fastify-server.mjs",
    "dev:m:express": "cross-env NODE_ENV=development node ./express-server.mjs",
    "start:m:fastify": "cross-env NODE_ENV=production node ./fastify-server.mjs",
    "start:m:express": "cross-env NODE_ENV=production node ./express-server.mjs"
  },
  "dependencies": {
    "@exortek/remix-express": "^1.0.8",
    "@exortek/remix-fastify": "^1.0.1",
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.0.1",
    "@remix-run/express": "^2.12.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "express": "^4.21.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.1",
    "isbot": "^4.1.0",
    "pretty-cache-header": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.12.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.4.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
